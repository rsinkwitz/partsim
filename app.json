{
  "expo": {
    "name": "padips",
    "slug": "padips",
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#546E7A"
    },
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "android": {
      "package": "com.rsinkwitz.padips",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#546E7A"
      },
      "userInterfaceStyle": "dark",
      "statusBar": {
        "translucent": true,
        "backgroundColor": "#00000000"
      }
    },
    "ios": {
      "bundleIdentifier": "com.anonymous.rscubesexpo",
      "icon": "./assets/icon.png"
    },
    "plugins": [
      "expo-asset"
    ]
  }
}
