{
  "expo": {
    "name": "padips",
    "slug": "padips",
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#b0c4de"
    },
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "android": {
      "package": "com.rsinkwitz.padips",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#b0c4de"
      }
    },
    "ios": {
      "bundleIdentifier": "com.anonymous.rscubesexpo",
      "icon": "./assets/icon.png"
    },
    "plugins": [
      "expo-asset"
    ]
  }
}
